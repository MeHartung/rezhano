{
    "name": "gpn/gpn-nvi",
    "license": "proprietary",
    "type": "project",
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/accurateweb/email-template-bundle.git"
    },{
        "type": "vcs",
        "url": "https://github.com/accurateweb/content-hotspot-bundle"
    }],
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "SynchronizationBundle\\": "SynchronizationBundle/"
        }
    },
    "require": {
        "php": ">=7.1",
        "symfony/symfony": "3.3.*",
        "doctrine/orm": "2.6.*",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "twig/extensions": " 1.5.*",
        "doctrine/doctrine-migrations-bundle": "^1.0",
        "stof/doctrine-extensions-bundle": "^1.2",
        "sonata-project/admin-bundle": "^3.18",
        "sonata-project/doctrine-orm-admin-bundle": "^3.1",
        "friendsofsymfony/user-bundle": "2.0.*",
        "jms/serializer-bundle": "^2.0",
        "beberlei/DoctrineExtensions": "^1.0",
        "pixassociates/sortable-behavior-bundle": "^1.1",
        "redcode/tree-bundle": "dev-master",
        "accurateweb/email-template-bundle": "dev-master",
        "phpunit/phpunit": "7.0.*",
        "twig/twig": "2.*",
        "accurateweb/content-hotspot-bundle": "dev-development",
        "stfalcon/tinymce-bundle": "1.0",
        "ext-intl": "^1.1",
        "tooyz/moysklad": "0.4.4"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "phpunit/phpunit": "^7.0",
        "deployer/deployer": "^6.1"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
